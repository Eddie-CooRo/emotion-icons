{
  "presets":[
    [
      "env",
      {
        "modules":false,
        "shippedProposals":true,
        "targets":{
          "browsers":[
            "last 2 versions",
            "ie >= 9"
          ]
        },
        "useBuiltIns":"usage"
      }
      ],"babel-preset-react",
      "babel-preset-stage-2"
  ],
  "plugins":[
    "babel-plugin-transform-export-extensions",
    [
      "babel-plugin-transform-runtime",
      {
        "helpers":true,
        "polyfill":false,
        "regenerator":false
      }
    ]
  ],
  "env":{
    "cjs":{
      "presets":[
        [
          "env",
          {
            "modules":"commonjs",

          }
        ]
      ]
    }
  }
}
