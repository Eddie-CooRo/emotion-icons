{
  "name": "emotion-icons",
  "version": "0.0.3",
  "description": "Font Awesome, Material Icons, and Octicons available as Emotion Components",
  "author": "Josh Rosenstein <jrosenstein6@gmail.com>",
  "homepage": "https://github.com/joshuarosenstein/emotion-icons",
  "repository": "git://github.com/joshuarosenstein/emotion-icons-emotion.git",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "sideEffects": false,
  "files": [
    "/fa-brands",
    "/fa-regular",
    "/fa-solid",
    "/feather",
    "/material",
    "/octicons",
    "/ion-ios",
    "/ion-md",
    "/simpleIcons",
    "/CHANGELOG.md",
    "/index.d.ts",
    "/index.cjs.js",
    "/index.js",
    "/LICENSE",
    "/package.json",
    "/README.md",
    "/manifest.json"
  ],
  "keywords": [
    "styled-components",
    "emotion",
    "icons",
    "font-awesome",
    "feather-icons",
    "material-ui",
    "material",
    "octicons",
    "svg"
  ],
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "node generate/generate.js",
    "test": "node generate/Test.js",
    "build:es": "cross-env BABEL_ENV=es babel build/src --out-dir build/icons --ignore *.spec.js,*.test.js ",
    "build:cjs": "cross-env BABEL_ENV=cjs babel build/src --out-dir build/icons-cjs --ignore *.spec.js,*.test.js ",
    "clean": "rimraf build fa-brands fa-regular fa-solid material octicons boxicons-solid boxicons-regular simpleIcons ion-ios ion-md feather manifest.json index.d.ts index.cjs.js index.js",
    "clean:modules": "rimraf node_modules"
  },
  "peerDependencies": {
    "emotion": "8 || 9",
    "react-emotion": "8 || 9"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "boxicons": "^1.6.0",
    "cross-env": "^5.2.0",
    "execa": "^0.10.0",
    "fast-glob": "^2.2.0",
    "feather-icons": "^4.7.0",
    "fs-extra": "^6.0.0",
    "h2x-core": "^0.1.9",
    "h2x-plugin-jsx": "^0.1.9",
    "ionicons": "^4.2.4",
    "js-search": "^1.4.2",
    "material-design-icons": "^3.0.1",
    "number-to-words": "^1.2.3",
    "octicons": "^7.2.0",
    "ora": "^2.0.0",
    "prettier": "^1.13.0",
    "rimraf": "^2.6.2",
    "serve": "^9.0.0",
    "simple-icons": "^1.6.4",
    "svgo": "^1.0.5"
  },
  "dependencies": {}
}
